{
  "expo": {
    "name": "Zentors",
    "slug": "Zentors",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./src/constants/google-services.json",
      "package": "com.zentors"
    },
    "androidStatusBar": {
      "hidden": false,
      "translucent": true,
      "barStyle": "dark-content",
      "backgroundColor": "#00000000"
    }
  }
}